datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int       @id @default(autoincrement())
  username  String
  password  String
  email     String
  token     String    @default("0")

  @@unique([email])
  @@unique([username, password, token], name: "userJwtCheck")
}
